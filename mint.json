{
  "$schema": "https://mintlify.com/schema.json",
  "name": "Glitch API",
  "logo": {
    "dark": "/images/logo/dark.svg",
    "light": "/images/logo/light.svg"
  },
  "favicon": "/favicon.svg",
  "colors": {
    "primary": "#0D9373",
    "light": "#07C983",
    "dark": "#0D9373",
    "anchors": {
      "from": "#0D9373",
      "to": "#07C983"
    }
  },
  "feedback": {
    "thumbsRating": true
  },
  "anchors": [
    {
      "name": "Glitch",
      "icon": "browser",
      "url": "https://glitch.com"
    },
    {
      "name": "Community",
      "icon": "discourse",
      "url": "https://support.glitch.com/"
    }
  ],
  "navigation": [
    {
      "group": "About",
      "pages": [
        "docs/introduction"
      ]
    },
    {
      "group": "Project endpoints",
      "pages": [
        "docs/project/project",
        "docs/project/readme",
        "docs/project/sleep",
        "docs/project/exec",
        "docs/project/setEnv",
        "docs/project/download",
        "docs/project/remix",
        "docs/project/archive",
        "docs/project/unarchive"
      ]
    },
    {
      "group": "User endpoints",
      "pages": [
        "docs/user/search",
        "docs/user/by-id",
        "docs/user/by-username",
        "docs/user/projects",
        "docs/user/deletedProjects",
        "docs/user/anon",
        "docs/user/hours"
      ]
    },
    {
      "group": "Collection endpoints",
      "pages": [
        "docs/collection/get",
        "docs/collection/new"
      ]
    },
    {
      "group": "Editor endpoints",
      "pages": [
        "docs/editor/logs",
        "docs/editor/terminal",
        "docs/editor/add-domain",
        "docs/editor/remove-domain",
        "docs/editor/list-domain",
        "docs/editor/add-member",
        "docs/editor/remove-member"
      ]
    }
  ]
}